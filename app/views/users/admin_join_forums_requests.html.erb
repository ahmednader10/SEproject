<p>Requests to join my Forums: </p>
<table>
  <tr>
    <th>Forum</th>
    <th>User</th>
    <th>Request</th>

  </tr>
 
  <% @requests.each do |request| %>
    <tr>
      <td>
      		<%= request.forum_id %>
      		<%= request.user_id  %>
      		<%= request.accept %>
      </td>
    </tr>
  <% end %>
</table>